<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="frontend.screens.controllers.SelectLetterSetController">
   <children>
      <AnchorPane fx:id="viewAP" prefHeight="300.0" prefWidth="600.0" style="-fx-background-color: #B08968;">
         <children>
            <ScrollPane fx:id="viewSP" prefHeight="300.0" prefWidth="600.0" style="-fx-background-color: #B08968;" />
         </children>
      </AnchorPane>
      <AnchorPane fx:id="buttonAP" layoutY="300.0" prefHeight="100.0" prefWidth="600.0" style="-fx-background-color: #9C6644;">
         <children>
            <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onMouseClicked="#save" prefHeight="72.0" prefWidth="183.0" style="-fx-background-color: #B08968;" text="Save" textFill="WHITE">
               <font>
                  <Font name="Consolas Bold" size="33.0" />
               </font>
            </Button>
            <Button layoutX="402.0" layoutY="14.0" mnemonicParsing="false" onMouseClicked="#goBack" prefHeight="72.0" prefWidth="183.0" style="-fx-background-color: #B08968;" text="Go Back" textFill="WHITE">
               <font>
                  <Font name="Consolas" size="33.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
